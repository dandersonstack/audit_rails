<div id='page-views' class='overlay col-sm-12'>
  <div data-attr-id="page-views" class="overlay-section">
    <div class="section-header">
      <div class="section-text">
        View by Pages
      </div>
      <button id="page-views-zoomout" type="button" class="btn btn-default btn-lg section-zoom">
        <span class='glyphicon glyphicon-remove large'></span>
      </button>
      <div class="section-count">
        <%= @analysis[:total] %>
      </div>
    </div>
  </div>
  <div id='pageViewsCount' data-visit-count='<%= @analysis[:by_page_views] %>' class='chart-modal'></div>
</div>
