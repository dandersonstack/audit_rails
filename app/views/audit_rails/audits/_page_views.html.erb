<div id='page-views' class='overlay'>
  <div data-attr-id="page-views" class="col-sm-12 overlay-section">
    <div class="section-header">
      <div class="section-text">
        View by Pages
      </div>
      <div class="section-count">
        <%= @total %>
      </div>
      <button id="page-views-zoomout" type="button" class="btn btn-default btn-lg section-zoom">
        <span class='glyphicon glyphicon-fullscreen large'></span>
      </button>
    </div>
  </div>
  <div id='pageViewsCount' data-visit-count='<%= @analysis_by_page_views %>'></div>
</div>
