<div id='user-pages' class='overlay col-sm-12'>
  <div data-attr-id="user-pages" class="overlay-section">
    <div class="section-header">
      <div class="section-text">
        Users' Views by Pages
      </div>
      <button id="user-pages-zoomout" type="button" class="btn btn-default btn-lg section-zoom">
        <span class='glyphicon glyphicon-remove large'></span>
      </button>
      <div class="section-count">
        <%= @analysis[:total] %>
      </div>
    </div>
  </div>
  <div id='per-user-by-pages-bar' class='overlay-section chart-modal'>
    <div class='left page-views left-navigation'>
      <div class="list-group page-views">
        <% JSON.parse(@analysis[:by_user_name]).each do |user_hsh| %>
          <a href="#page-views-by-user" class="list-group-item"
            data-user='<%=user_hsh["user"]%>'
            data-page-visits='<%= page_visits_by_user(user_hsh['user'])%>'>
            <%= user_hsh["user"] %> - <%= percentage_share(user_hsh["user"], user_hsh["count"], @analysis[:total])%>
            <span class="badge"><%= user_hsh["count"] %></span>
          </a>
        <% end %>
      </div>
    </div>
    <div class='left'>
      <div id="userNameToShow"></div>
      <div id='pageViewsByUser'></div>
      <div id='pageViewsShareByUser'></div>
    </div>
    <div id="tooltip"></div>
  </div>
</div>
