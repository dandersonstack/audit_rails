<div id='hourly-views' class='overlay col-sm-12'>
  <div data-attr-id="hourly-views" class="overlay-section">
    <div class="section-header">
      <div class="section-text">
        Hourly Views (UTC)
      </div>
      <button id="hourly-views-zoomout" type="button" class="btn btn-default btn-lg section-zoom">
        <span class='glyphicon glyphicon-remove large'></span>
      </button>
      <div class="section-count">
        <%= @analysis[:total] %>
      </div>
    </div>
  </div>

  <div id='hourlyViewsCount' data-visit-count='<%= @analysis[:by_hourly_views] %>' class='chart-modal'></div>
</div>
